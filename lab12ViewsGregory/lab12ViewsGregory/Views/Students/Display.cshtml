<link rel="stylesheet" href="~/StyleSheet.css">

@model List<lab12ViewsGregory.ViewModels.Student>
@{

}
<h1>Student Portal</h1>
<h2>List of currently enrolled students</h2>
@foreach(var s in @Model)
{
    <ul>
        <li>@s.LastName, @s.FirstName</li>
        <li>Grade: @s.Grade</li>
        <li>Skills: @s.SkillSet</li>
    </ul>
}